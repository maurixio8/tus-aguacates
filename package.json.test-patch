    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "jest": "^29.7.0",
    "jsdom": "^23.0.1",
    "msw": "^2.0.11",
    "playwright": "^1.40.1",
    "vitest": "^1.0.4"
  }
}